{
  "name": "@stone-flower-org/react-template",
  "version": "1.0.0",
  "description": "React template",
  "main": "src/index.tsx",
  "repository": "https://github.com/stone-flower-org/react-template",
  "author": "stone-flower-dg",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean:build && vite build",
    "clean": "yarn clean:build && yarn clean:report",
    "clean:build": "rm -rf ./dist",
    "clean:report": "rm -rf ./report",
    "lint": "biome check .",
    "lint:format": "biome check --write .",
    "lint:staged": "lint-staged",
    "prepare": "./scripts/prepare.sh",
    "proxy": "node ./scripts/proxy.js",
    "start": "vite",
    "test": "vitest run",
    "test:coverage": "vitest run --silent --coverage",
    "typecheck": "tsc --noEmit --project ./tsconfig.json"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,html}": ["biome check"]
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@emotion/jest": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitest/coverage-v8": "2.1.2",
    "esprima": "^4.0.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.10",
    "vitest": "2.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/base": "^5.0.0-alpha.113",
    "@mui/material": "^5.11.4",
    "@mui/x-date-pickers": "^6.3.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@stone-flower-org/js-app": "^0.1.0",
    "@stone-flower-org/js-utils": "^0.1.0",
    "@stone-flower-org/vite-plugin-copy-static": "^0.1.0",
    "@stone-flower-org/vite-plugin-html-template": "^0.1.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.25",
    "@vitejs/plugin-react": "^4.2.0",
    "axios": "^1.3.2",
    "emotion-ts-plugin": "^2.0.1",
    "history": "^5.3.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.27.0",
    "recharts": "^2.6.2",
    "sass": "^1.94.2",
    "sass-embedded": "^1.80.6",
    "typescript": "^4.9.4",
    "vite": "5.0.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
